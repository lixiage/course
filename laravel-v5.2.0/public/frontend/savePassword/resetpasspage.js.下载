define(function(require){require("common"),require("/static/component/base/placeholder/placeholder.js"),require("/static/component/base/util/validate.js");var a=$("#pagePwReset");a.validateSetup({fields:{newpass:{blur:function(){$(this).validate(),$(this.form).find("[name='confirm']").validate()}},confirm:{rules:[{rule:function(a,c){return $.trim($(this.form).find("[name='newpass']").val())!==c?"两次密码输入不一致！":void 0},force:1}],keyup:function(e){"13"==e.keyCode?$("#reset-submit").trigger("click"):$(this).validate()}}},onerror:function(e){var a,c;e._relateField&&e.tip&&(a=$(e._relateField),a.addClass("rlf-field-error").next(".rlf-tip-wrap").html(e.tip).addClass("rlf-tip-error"),(c=a.data("placeholder-textinput"))&&c.addClass("rlf-field-error"))},onvalid:function(e){var a,c;e._relateField&&(a=$(e._relateField),a.removeClass("rlf-field-error").next(".rlf-tip-wrap").removeClass("rlf-tip-error").empty(),(c=a.data("placeholder-textinput"))&&c.removeClass("rlf-field-error")),$("#forgetpwd-globle-error").removeClass("rlf-tip-error").empty()}}),a.find("input[placeholder]").placeholder(),$("#reset-submit").on("click",function(){var a=$(this);a.closest("form").validate({success:function(){this.submit()},error:function(){}})})});